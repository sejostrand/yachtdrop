{
  "kind": "collectionType",
  "collectionName": "suppliers",
  "info": {
    "name": "suppliers"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "supplierName": {
      "type": "string",
      "unique": true
    },
    "supplierEmail": {
      "type": "email"
    },
    "contactNumber": {
      "type": "biginteger"
    },
    "address": {
      "type": "richtext"
    },
    "products": {
      "via": "supplier",
      "collection": "products"
    },
    "ports": {
      "via": "suppliers",
      "collection": "ports"
    }
  }
}
