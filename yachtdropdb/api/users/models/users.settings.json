{
  "kind": "collectionType",
  "collectionName": "users",
  "info": {
    "name": "Users"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "user_email": {
      "type": "email",
      "unique": true
    },
    "mobile_number": {
      "type": "biginteger"
    },
    "user_password": {
      "type": "password"
    },
    "recovery_password": {
      "type": "password"
    },
    "orders": {
      "via": "user",
      "collection": "orders"
    }
  }
}
