{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "orders",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "state": {
      "type": "enumeration",
      "enum": [
        "cart",
        "active",
        "cancelled",
        "completed"
      ],
      "default": "cart",
      "required": true
    },
    "order_id": {
      "type": "string",
      "required": true
    },
    "products": {
      "type": "json"
    },
    "transaction_id": {
      "type": "string"
    },
    "user": {
      "type": "string"
    },
    "order_datetime": {
      "type": "datetime"
    },
    "expected_delivery": {
      "type": "datetime"
    }
  }
}
